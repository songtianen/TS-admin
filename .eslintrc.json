{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "camelcase": [0],
    "global-require": [0],
    "import/no-extraneous-dependencies": [0],
    "no-unused-expressions": [0],
    "no-useless-escape": [0],
    "semi": [0],
    "no-debugger": [0],
    "no-empty": [0],
    "comma-dangle": [0],
    "space-before-function-paren": [0],
    "import/no-cycle": [0],
    "max-len": [0]
  }
}
