 {"parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
    "extends": ["airbnb"],
"rules":{
"import/no-extraneous-dependencies":[0],
"no-plusplus":[0],
"no-param-reassign":[0],
"no-confusing-arrow":[0],
"implicit-arrow-linebreak":[0],
"arrow-body-style":[0],
"no-restricted-syntax":[0],
"operator-linebreak":[0],
"consistent-return":[0],
"no-shadow":[0],
"no-await-in-loop":[0],
"object-curly-newline":[0],
"no-underscore-dangle":[0],
"guard-for-in":[0],
"global-require":[0]
}
  }
